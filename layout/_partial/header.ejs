<div id="kratos-wrapper">
    <div id="kratos-page">
        <div id="kratos-header">
            <header id="kratos-desktop-topnav" class="kratos-topnav">
                <div class="container">
                    <div class="nav-header">
                        <nav id="kratos-menu-wrap">
                            <ul id="kratos-primary-menu" class="sf-menu">
                                <% if (theme.topMenu) { %>
                                    <% for (const menu of theme.topMenu) { %>
                                        <li>
                                            <% if (menu.submenu) { %>
                                                <a>
                                            <% } else { %>
                                                <a href="<%- url_for(menu.url) %>">
                                            <% } %>
                                                <% if (menu.icon) { %>
                                                    <i class="fa fa-<%- menu.icon %>"></i>
                                                <% } %>
                                                <%- menu.label %>
                                            </a>
                                            <% if (menu.submenu) { %>
                                                <ul class="sub-menu">
                                                    <% for (const submenu of menu.submenu) { %>
                                                        <li>
                                                            <a href="<%- url_for(submenu.url) %>">
                                                                <% if (submenu.icon) { %>
                                                                    <i class="fa fa-<%- submenu.icon %>"></i>
                                                                <% } %>
                                                                <%- submenu.label %>
                                                            </a>
                                                        </li>
                                                    <% } %>
                                                </ul>
                                            <% } %>
                                        </li>
                                    <% } %>
                                <% } else { %>
                                    <% /* Fallback to classic settings */ %>
                                    <% for (const i in theme.menu){ %>
                                        <% if (typeof (theme.menu[i]['submenu']) !== 'undefined') { %>
                                            <li>
                                                <a><%- theme.label[i]['submenu'] %></a>
                                                <ul class="sub-menu">
                                                    <% for (const j in theme.menu[i]){ %>
                                                        <% if (j !== 'submenu') { %>
                                                            <li><a href="<%- url_for(theme.menu[i][j]) %>"><%- theme.label[i][j] %></a></li>
                                                        <% } %>
                                                    <% } %>
                                                </ul>
                                            </li>
                                        <% } else { %>
                                            <li><a href="<%- url_for(theme.menu[i]) %>"><%- theme.label[i] %></a></li>
                                        <% } %>
                                    <% } %>
                                <% } %>
                            </ul>
                        </nav>
                    </div>
                </div>
            </header>
            <header id="kratos-mobile-topnav" class="kratos-topnav">
                <div class="container">
                    <div class="color-logo"><a href="<%- url_for() %>"><%= config.title %></a></div>
                    <div class="nav-toggle">
                        <a class="kratos-nav-toggle js-kratos-nav-toggle">
                            <i></i>
                        </a>
                    </div>
                </div>
            </header>
        </div>
        <div class="kratos-start kratos-hero-2">
            <!-- <div class="kratos-overlay"></div> -->
            <div class="kratos-cover kratos-cover-2 text-center">
                <div class="desc desc2 animate-box">
                    <a href="<%- url_for() %>">
                        <h2><%= config.title %></h2> <br />
                        <span><%= config.subtitle %></span>
                    </a>
                </div>
            </div>
        </div>

        <div id="kratos-blog-post">
            <div class="container">
                <div id="main" class="row">
                    <% if (page.type !== '404' && theme.sidebar === 'left') { %>
                        <%- partial('_partial/sidebar') %>
                    <% } %>